{
  "extends": "airbnb",

  "env": {
    "browser": true
  },

  "rules": {
    "consistent-return": 0,
    "func-names": 0,
    "max-len": 0,
    "no-continue": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "prefer-destructuring": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "prefer-template": 0,
    "strict": 0,
    "vars-on-top": 0
  }
}
